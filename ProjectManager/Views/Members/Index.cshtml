@using ProjectManager.Models;
@model IEnumerable<ProjectManager.Models.Members>
@{
    ViewBag.Title = "會員";
}

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>會員帳號</th>
            <th>員工名稱</th>
            <th>權限等級</th>
            <th>新增日期</th>
            <th>修改日期</th>
            <th>最後登入日期</th>
            <th>修改</th>
            <th>刪除帳號</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var _member in (List<Members>)ViewBag.members)
        {
            <tr>
                <td>@_member.MemberID</td>
                <td>@_member.Employee.EmployeeName</td>
                <td>@_member.Permissions.PermissionsName</td>
                <td>@_member.CreateDate</td>
                <td>@_member.ModifiedDate</td>
                <td>@_member.LastLoginDate</td>
                <td><a href="~/Members/Edit/@_member.MemberGUID" class="btn btn-primary">修改</a></td>
                <td><a href="~/Members/Delete/@_member.MemberGUID" class="btn btn-danger">刪除帳號</a></td>
            </tr>
        }
    </tbody>
    @*<a href="~/Members/Edit/@_member.PermissionsGUID" class="btn btn-primary">修改</a>*@
</table>


